        {
          "label": "text_Gender",
          "placeholder": "text_Gender",
          "id": "gender",
          "key": "genderMaleCheckbox,genderFemaleCheckbox",
          "control": "select",
          "options": [
            {
              "value": "Male",
              "label": "text_Male",
              "keyValue": "true,false"
            },
            {
              "value": "Female",
              "label": "text_Female",
              "keyValue": "false,true"
            }
          ]
        },
        {
          "label": "text_Race",
          "placeholder": "text_Race",
          "id": "race",
          "key": "race",
          "control": "select",
          "options": "RaceOptions"
        },
        {
          "label": "text_Religion",
          "placeholder": "text_Religion",
          "id": "religion",
          "key": "religion",
          "control": "select",
          "options": "ReligionOptions"
        }
      ]
    },
    {
      "label": "text_Language",
      "id": "language",
      "control": "group",
      "maxChildPerRow": "3",
      "fields": [
        {
          "label": "text_NativeLanguage",
          "placeholder": "text_NativeLanguage",
          "id": "native_language",
          "key": "nativeLanguage",
          "control": "text"
        },
        {
          "label": "text_FluentInEnglish",
          "id": "fluent_in_english",
          "key": "fluentEnglishYesCheckbox,fluentEnglishNoCheckbox",
          "control": "checkbox",
          "options": [
            {
              "value": "true",
              "label": "text_Yes",
              "keyValue": "true,false"
            },
            {
              "value": "false",
              "label": "text_No",
              "keyValue": "false,true"
            }
          ]
        }
      ]
    },
    {
      "label": "text_OtherFluentLanguagesLabel",
      "placeholder": "text_OtherFluentLanguages",
      "id": "other_fluent_languages",
      "key": "otherFluentLanguages",
      "control": "component_textbox_na"
