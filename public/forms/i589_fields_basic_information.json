          "key": "cityAndCountryOfBirth",
          "control": "component_location_dropdown",
          "className": "single-line"
        }
      ]
    },
    {
      "label": "text_Nationality",
      "id": "nationality",
      "control": "group",
      "maxChildPerRow": "3",
      "fields": [
        {
          "label": "text_NationalityAtBirth",
          "placeholder": "text_NationalityAtBirth",
          "id": "nationality_at_birth",
          "key": "nationalityAtBirth",
          "control": "select",
          "options": "NationalityOptions"
        },
        {
          "label": "text_PresentNationality",
          "placeholder": "text_PresentNationality",
          "id": "nationality",
          "key": "nationality",
          "control": "select",
          "options": "NationalityOptions"
        }
      ]
    },
