          "placeholder": "text_Gender",
          "id": "gender",
          "key": "applicant.genderMaleCheckbox,applicant.genderFemaleCheckbox",
          "control": "select",
          "options": [
            {
              "value": "Male",
              "label": "text_Male",
              "keyValue": "true,false"
