        {
          "label": "text_NationalityAtBirth",
          "placeholder": "text_NationalityAtBirth",
          "id": "nationality_at_birth",
          "key": "applicant.nationalityAtBirth",
          "control": "select",
          "options": "NationalityOptions"
        }
      ]
