          "control": "text"
        }
      ]
    },
    {
      "label": "text_Name",
      "id": "name",
      "control": "group",
      "maxChildPerRow": "3",
      "fields": [
        {
          "label": "text_FirstName",
          "placeholder": "text_FirstName",
          "id": "first_name",
          "key": "firstName",
          "control": "text"
        },
        {
          "label": "text_MiddleName",
          "placeholder": "text_MiddleName",
          "id": "middle_name",
          "key": "middleName",
          "control": "text"
        },
        {
          "label": "text_LastName",
          "placeholder": "text_LastName",
          "id": "last_name",
          "key": "lastName",
          "control": "text"
        }
